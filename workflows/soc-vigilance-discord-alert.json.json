      "parameters": {
        "jsCode": "return [\n  {\n    json: {\n      fingerprint: `${$json.src_ip}-${$json.dst_host}-${$json.rule}`,\n      source: $json.source,\n      alert_id: $json.alert_id,\n      rule: $json.rule,\n      severity: Number($json.severity),\n      src_ip: $json.src_ip,\n      dst_host: $json.dst_host,\n      event_time: new Date($json.timestamp).getTime()\n    }\n  }\n];\n"
      },
