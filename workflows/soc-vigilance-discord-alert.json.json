      "name": "Normalize_And_Fingerprint"
    },
    {
      "parameters": {
        "jsCode": "// 10-minute deduplication window\nconst DEDUP_WINDOW_MS = 10 * 60 * 1000;\n\nconst fingerprint = $json.fingerprint;\nconst now = Date.now();\n\n// âœ… Correct API for Code node\nconst staticData = $getWorkflowStaticData('global');\n\nconst lastSeen = staticData[fingerprint];\n\nif (lastSeen && (now - lastSeen < DEDUP_WINDOW_MS)) {\n  // Duplicate â†’ DROP\n  return [];\n}\n\n// Store timestamp and allow alert\nstaticData[fingerprint] = now;\nreturn [{ json: $json }];\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        -624,
        96
      ],
      "id": "d1f4fe76-c33e-4345-afcf-d088104c4234",
      "name": "Deduplication_Engine"
    },
    {
      "parameters": {
        "jsCode": "let score = 0;\n\n// Severity-based weight\nif ($json.severity >= 9) {\n  score += 50;\n} else if ($json.severity >= 7) {\n  score += 40;\n} else if ($json.severity >= 5) {\n  score += 20;\n}\n\n// Behavior-based keywords\nconst rule = $json.rule.toLowerCase();\n\nif (rule.includes(\"brute\")) score += 20;\nif (rule.includes(\"lateral\")) score += 30;\nif (rule.includes(\"ransom\")) score += 40;\nif (rule.includes(\"exfil\")) score += 40;\n\n// Off-hours context (UTC)\nconst hour = new Date($json.event_time).getUTCHours();\nif (hour >= 0 && hour <= 5) score += 10;\n\n// Attach score\n$json.risk_score = score;\n\nreturn [{ json: $json }];\n"
      },
